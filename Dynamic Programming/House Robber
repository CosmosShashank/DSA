// Leet code link : https://leetcode.com/problems/house-robber/post-solution/?submissionId=1261160351
public class Solution {
    public int Rob(int[] nums) {
       
       if(nums.Length ==1)
       return nums[0];

        int f=0;
        int s= nums[0];
        int c=0;
                  
        for(int i=2;i<=nums.Length;i++)
         {
          c = Math.Max(nums[i-1]+ f,s);  
          f=s;
          s=c;
         }

         return c;
    }
}
